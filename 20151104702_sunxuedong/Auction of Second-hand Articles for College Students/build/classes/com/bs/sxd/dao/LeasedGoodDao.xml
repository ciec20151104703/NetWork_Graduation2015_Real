<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
     "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.bs.sxd.dao.LeasedGoodDao">

	<select id="selectLeasedGoodList" parameterType="leasedgoods" 
                                           resultType="leasedgoods">
	SELECT
	*
	FROM
	leasedgoods	
	<include refid="selectLeasedGoodListWhere"/>
		<!-- 执行分页查询 -->
		<if test="start !=null and rows != null">
			limit #{start},#{rows}
		</if>
	</select>
</mapper>